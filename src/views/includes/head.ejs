<script>
    localStorage.getItem('theme') === 'dark' ? document.querySelector('html').setAttribute('data-bs-theme', 'dark') : document.querySelector('html').removeAttribute('data-bs-theme', 'dark')
    // Funcioﾌ］ para abrir el chat
    const openChat = () => {
        window.location.href = '/chat';      
    }

    // Funcioﾌ］ para cerrar el chat
    const closeChat = () => {
        window.location.href = '/';      
    }

    // cambiar el tema
    const changeTheme = (x) => {
        console.log(x);
        const html = document.querySelector('html')
        if (x) {
            html.setAttribute('data-bs-theme', 'dark')
            localStorage.setItem('theme', 'dark')
        }else{
            html.removeAttribute('data-bs-theme', 'dark')
            localStorage.setItem('theme', 'light')
        }
    }
</script>